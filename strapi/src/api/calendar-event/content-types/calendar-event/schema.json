      "type": "datetime",
      "required": true
    },
    "endDate": {
      "type": "datetime",
      "required": true
    },
    "color": {
      "type": "enumeration",
      "default": "blue",
      "enum": [
        "blue",
        "green",
        "red",
        "yellow",
        "purple",
        "orange"
      ]
    },
    "description": {
      "type": "text"
    }
  }
}
